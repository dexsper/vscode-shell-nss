{
  "name": "shell-nss-syntax-highlighting",
  "displayName": "Shell NSS Syntax Highlighting",
  "description": "Syntax highlighting for Shell .nss configuration files",
  "version": "0.0.1",
  "publisher": "dexsper",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "shell-nss",
        "aliases": [
          "Shell NSS",
          "shell-nss"
        ],
        "extensions": [
          ".nss"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "shell-nss",
        "scopeName": "source.shell-nss",
        "path": "./syntaxes/shell-nss.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -b",
    "watch": "tsc -b -w",
    "lint": "eslint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dexsper/vscode-shell-nss.git"
  },
  "homepage": "https://github.com/dexsper/vscode-shell-nss#readme",
  "bugs": {
    "url": "https://github.com/dexsper/vscode-shell-nss/issues"
  },
  "devDependencies": {
    "typescript": "^4.1.3",
    "@types/node": "^14.14.31",
    "@types/vscode": "^1.100.0",
    "eslint": "^9.13.0",
    "@eslint/js": "^9.13.0",
    "@stylistic/eslint-plugin": "^2.9.0",
    "typescript-eslint": "^8.39.0"
  }
}
